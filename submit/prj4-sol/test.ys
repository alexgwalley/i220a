# Execution begins at address 0 
        .pos    0
       
init:   
				irmovq stack, %rsp
				call test					 # 0
        halt               # terminate program 

test:
	irmovq $10, %rax
	irmovq $20, %rbx
	subq %rbx, %rax
	jle done
	irmovq $2, %rcx
done:
	irmovq $3, %rdx
	ret


#stack starts here and grows to lower addresses  
       .pos   0x200
stack:
